        {
          "in": "query",
          "name": "id-optional",
          "schema": {
            "type": "number",
            "minimum": 10,
            "maximum": 20,
            "multipleOf": 2,
            "default": 12,
            "example": 14
          },
          "required": false
        },
        {
          "in": "query",
          "name": "id-exclusive-required",
          "schema": {
            "type": "number",
            "exclusiveMinimum": 10,
            "exclusiveMaximum": 20,
            "multipleOf": 2,
            "default": 12,
            "example": 14
          },
          "required": true
        },
        {
          "in": "query",
          "name": "id-exclusive-required-schema-v4",
          "description": "This is the default for anything pre 3.1",
          "schema": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "type": "number",
            "minimum": 10,
            "maximum": 20,
            "exclusiveMinimum": true,
            "exclusiveMaximum": true,
            "multipleOf": 2,
            "default": 12,
            "example": 14
          },
          "required": true
        },
        {
          "in": "query",
          "name": "id-exclusive-required-schema-v5",
          "description": "This isn't actually a valid version. See https://json-schema.org/draft-06/json-schema-release-notes.html#q-what-happened-to-draft-05",
          "schema": {
            "$schema": "https://json-schema.org/draft-05/schema#",
            "type": "number",
            "minimum": 10,
            "maximum": 20,
            "exclusiveMinimum": true,
            "exclusiveMaximum": true,
            "multipleOf": 2,
            "default": 12,
            "example": 14
          },
          "required": true
        },
        {
          "in": "query",
          "name": "id-exclusive-required-schema-v6",
          "schema": {
            "$schema": "https://json-schema.org/draft-06/schema#",
            "type": "number",
            "minimum": 10,
            "maximum": 20,
            "exclusiveMinimum": true,
            "exclusiveMaximum": true,
            "multipleOf": 2,
            "default": 12,
            "example": 14
          },
          "required": true
