    "/anything/numbers": {
      "parameters": [
        {
          "in": "query",
          "name": "id-required",
          "schema": {
            "type": "number",
            "minimum": 10,
            "maximum": 20,
            "multipleOf": 2,
            "default": 12,
            "example": 14
